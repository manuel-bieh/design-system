{
  "name": "@manuel-bieh/design-system",
  "description": "My personal little design system",
  "version": "0.0.3",
  "private": false,
  "author": "Manuel Bieh",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build0": "tsup --onSuccess \"yarn link:self\"",
    "build": "tsup",
    "dev": "concurrently \"yarn build --watch\" \"yarn storybook\" \"yarn test\" ",
    "link:self": "yarn yalc publish && yarn link",
    "lint": "eslint --ignore-path .gitignore \"{src,tests}/**/*.+(ts|js|tsx)\"",
    "lint:fix": "yarn lint --fix && prettier --write .",
    "prepare": "husky install",
    "release": "yarn build && yarn release-it",
    "start": "yarn dev",
    "storybook:build": "storybook build",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "ts": "node node_modules/.bin/ts-node -r tsconfig-paths/register -O '{\"module\": \"commonjs\", \"moduleResolution\": \"classic\", \"resolveJsonModule\": false }' --transpile-only --project ./tsconfig.json",
    "type-check": "tsc"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "./dist/index.css": "./dist/index.css",
    "./dist/*": "./dist/*",
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "./{src,tests}/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix"
    ],
    "*.{js,jsx,ts,tsx,css,md,json}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): v${version} --skip-ci"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@commitlint/cli": "18.4.4",
    "@commitlint/config-conventional": "18.4.4",
    "@csstools/postcss-global-data": "^2.1.1",
    "@ryansonshine/commitizen": "4.2.8",
    "@ryansonshine/cz-conventional-changelog": "3.3.4",
    "@storybook/addon-essentials": "7.6.10",
    "@storybook/addon-interactions": "7.6.10",
    "@storybook/addon-links": "7.6.10",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "7.6.10",
    "@storybook/react": "7.6.10",
    "@storybook/react-webpack5": "7.6.10",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/react": "14.1.2",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/react-test-renderer": "18.0.7",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "1.2.1",
    "autoprefixer": "^10.4.17",
    "babel-loader": "9.1.3",
    "classnames": "^2.5.1",
    "concurrently": "8.2.2",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-class-members": "1.19.0",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-typescript-sort-keys": "3.1.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "jsdom": "23.2.0",
    "lint-staged": "15.2.0",
    "postcss": "^8.4.33",
    "postcss-custom-media": "^10.0.2",
    "postcss-each": "^1.1.0",
    "postcss-nested": "^6.0.1",
    "prettier": "3.2.4",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "release-it": "17.0.1",
    "storybook": "7.6.10",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "typescript-plugin-css-modules": "^5.0.2",
    "vitest": "1.2.1",
    "yalc": "1.0.0-pre.53"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "resolutions": {
    "glob-parent": ">=5.1.2",
    "parse-url": ">=8.1.0",
    "semver": ">=7.5.2",
    "trim": ">=0.0.3",
    "trim-newlines": ">=3.0.1",
    "yaml": ">=2.2.2"
  }
}
